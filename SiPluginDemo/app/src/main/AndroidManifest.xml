<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sitech.paas.siplugindemo">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 获取ip -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
            android:name=".CrashApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.example.sitechapm.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.sitechapm.activity.ClickEffectActivity"/>
        <activity android:name="com.example.sitechapm.activity.CrashActivity"/>
        <activity android:name="com.example.sitechapm.activity.DatabaseActivity"/>
        <activity android:name="com.example.sitechapm.activity.DeviceInfoActivity"/>
        <activity android:name="com.example.sitechapm.activity.HttpUrlConnectionActivity"/>
        <activity android:name="com.example.sitechapm.activity.Okhttp2Activity"/>
        <activity android:name="com.example.sitechapm.activity.Okhttp3Activity"/>
        <activity android:name="com.example.sitechapm.activity.PageTraceActivity"/>
        <activity android:name="com.example.sitechapm.activity.WebViewActivity"/>
        <activity android:name="com.example.sitechapm.activity.WebviewListActivity"/>
    </application>

</manifest>